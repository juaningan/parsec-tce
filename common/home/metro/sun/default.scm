(define r 
  (lambda (addr) 
    (begin 
      (display (read-word addr))
      (newline))))

(define c 
  (lambda (addr) 
    (begin 
      (display (read-cword addr))
      (newline))))

(define b 
  (lambda (addr) 
    (begin 
      (if (= 0 (read-bit addr))
	  (display 0)
	  (display 1))
      (newline))))

(define w 
  (lambda (addr val) 
    (write-word addr val)))

(define ro 
  (lambda (addr len) 
      (begin 
	(define (display-next obj idx max)
	  (if (< idx max)
	      (begin
		(display (object-get-word obj idx))
		(display " ")
		(display-next obj (+ idx 1) max))))
	(define obj (object-new addr len))
	(object-read obj)
	(display-next obj 0 len)
	(object-destroy obj)
	(newline))))

(define s 
  (lambda (addr size)
    (begin
      (define scn (scan-new 100 
			    (lambda (sobj obj word)
			      (begin
				(display "Cambio de ")
				(display (scan-prev-word sobj word))
				(display " a ")
				(display (scan-get-word sobj word))
				(newline)))))
      (scan-object-add scn (object-new addr size)))))

